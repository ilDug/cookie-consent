{
    "name": "@ildug/cookie-consent",
    "version": "3.1.0",
    "description": "banner for cookies law, and proof of consent",
    "main": "index.ts",
    "scripts": {
        "tsc": "tsc",
        "tscdev": "npm run tsc --watch",
        "dev": "webpack --config webpack.config.js --watch --mode=development",
        "build": "webpack --config webpack.config.js --mode=production",
        "serve": "lite-server -c configs/lite-server/bs-config-open.json",
        "serve:test": "lite-server -c configs/lite-server/bs-config-background.json",
        "cy:open": "cypress open",
        "cy:run": "cypress run --browser=chrome",
        "cy:ci": "start-server-and-test serve:test http://localhost:8080 cy:run"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ilDug/cookie-consent.git"
    },
    "keywords": [
        "banner",
        "cookies",
        "privacy",
        "GDPR",
        "consent"
    ],
    "author": "DagTech",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ilDug/cookie-consent/issues"
    },
    "homepage": "https://github.com/ilDug/cookie-consent#readme",
    "dependencies": {
        "js-cookie": "^3.0.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/js-cookie": "^3.0.3",
        "@types/js-yaml": "^4.0.5",
        "@types/react": "^18.2.21",
        "@types/react-dom": "^18.2.7",
        "@types/uuid": "^8.3.4",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.8.1",
        "cypress": "^10.11.0",
        "file-loader": "^6.2.0",
        "lite-server": "^2.6.1",
        "mini-css-extract-plugin": "^2.7.6",
        "npm-run-all": "^4.1.5",
        "sass": "^1.66.1",
        "sass-loader": "^13.3.2",
        "start-server-and-test": "^1.15.4",
        "style-loader": "^3.3.3",
        "ts-loader": "^9.4.4",
        "typescript": "^4.9.5",
        "webpack": "^5.88.2",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.15.1"
    }
}
